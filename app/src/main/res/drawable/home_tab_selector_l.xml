<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="#ffffff" />
            <stroke android:width="1dip" android:color="#8bd3d3" />
            <corners android:topLeftRadius="5.0dip" android:topRightRadius="0dip"
                android:bottomLeftRadius="5.0dip" android:bottomRightRadius="0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#8bd3d3" />
            <stroke android:width="1dip" android:color="#8bd3d3" />
            <corners android:topLeftRadius="5.0dip" android:topRightRadius="0dip"
                android:bottomLeftRadius="5.0dip" android:bottomRightRadius="0dip" />
        </shape>
    </item>
</selector>